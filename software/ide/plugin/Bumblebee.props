# Properties file for Bumblebee plugin
# $Id$

# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
#plugin.BumblebeePlugin.activate=defer
plugin.BumblebeePlugin.activate=startup

# These two properties are required for all plugins
plugin.BumblebeePlugin.name=Bumblebee
plugin.BumblebeePlugin.author=Bumblebee Development Team

# Even if you don't store additional files, this is a good idea to set:
plugin.BumblebeePlugin.usePluginHome=true

# version number == jEdit version number
plugin.BumblebeePlugin.version=1.0

# online help
plugin.BumblebeePlugin.docs=index.html

# List dependencies of plugins with .depend.N property suffix:
plugin.BumblebeePlugin.depend.0=jdk 1.6
# See jEdit.getBuild() to understand version numbering scheme.
plugin.BumblebeePlugin.depend.1=jedit 05.00.01.00

plugin.BumblebeePlugin.description=This plugin provides support for the Bumblebee board.
# plugin menu
plugin.BumblebeePlugin.menu=bumblebee \
	- \
	bumblebee.choose-file \
	bumblebee.save-file \
	bumblebee.copy-to-buffer

# action labels for actions supplied by dockables.xml
bumblebee.label=Bumblebee

# action labels for actions supplied by actions.xml
bumblebee.choose-file.label=Choose notepad file
bumblebee.save-file.label=Save notepad file
bumblebee.copy-to-buffer.label=Copy notepad to buffer

# plugin option pane
plugin.BumblebeePlugin.option-pane=bumblebee

# Option pane activation BeanShell snippet
options.bumblebee.code=new BumblebeeOptionPane();

# Option pane labels
options.bumblebee.label=Bumblebee
options.bumblebee.choose-file=Choose
options.bumblebee.choose-file.title=Choose a notepad file
options.bumblebee.choose-font=Font:
options.bumblebee.show-filepath.title=Display notepad file path
options.bumblebee.gcc-label=Compiler Location:
options.bumblebee.library-label=Library Location:
options.bumblebee.prog-label=Interpreter Location:

# window title
bumblebee.title=Bumblebee

# window toolbar buttons
bumblebee.choose-file.icon=Open.png
bumblebee.save-file.icon=Save.png
bumblebee.copy-to-buffer.icon=CopyToBuffer.png

# default settings
options.bumblebee.show-filepath=true
options.bumblebee.font=Monospaced
options.bumblebee.fontstyle=0
options.bumblebee.fontsize=14

# Setting not defined but supplied for completeness
options.bumblebee.gcc-filepath=
options.bumblebee.library-filepath=
options.bumblebee.programmer-filepath=
